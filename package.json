{
  "name": "v-echarts",
  "version": "1.0.0",
  "author": "Vivien",
  "license": "MIT",
  "description":"v-echarts",
  "workspaces": [
    "packages/*",
    "docs"
  ],
  "main": "./index.js",
  "module": "./index.mjs",
  "exports": {
    ".": {
      "require": "./index.js",
      "import": "./index.mjs"
    },
    "./common-charts": {
      "require": "./packages/common-charts/dist/index.cjs.js",
      "import": "./packages/common-charts/dist/index.es.js"
    }
  },
  "scripts": {
    "docs:dev": "pnpm run -C docs docs:dev",
    "docs:build": "pnpm run -C docs docs:build && pnpm run -C packages/common-charts docs:build",
    "build": "pnpm run build:common",
    "build:common": "pnpm run -C packages/common-charts build",
    "docs:deploy": "pnpm run build && pnpm run docs:build"
  },
  "keywords": [
    "v-echarts",
    "vue",
    "element-plus"
  ],
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@vueuse/core": "^10.11.0",
    "element-plus": "^2.7.7",
    "less": "^4.2.0",
    "path": "^0.12.7",
    "prismjs": "^1.29.0",
    "vue": "^3.4.31"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.5",
    "postcss": "^8.4.39",
    "typescript": "^5.2.2",
    "vite": "^5.3.4",
    "vitepress": "^1.3.1",
    "vue-tsc": "^2.0.24"
  }
}